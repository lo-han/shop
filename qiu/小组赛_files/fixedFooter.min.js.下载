function getWinHeight(){var a=640;if(window.innerHeight){a=window.innerHeight}else{if(document.body&&document.body.clientHeight){a=document.body.clientHeight}else{if(document.documentElement&&document.documentElement.clientHeight){a=document.documentElement.clientHeight}}}return a}function setFooter(){var b=document.getElementById("j_wFooter");b.style.marginTop="0px";var a=getWinHeight(),c=document.body.offsetHeight;if(a>c){b.style.marginTop=(a-c)+"px"}b.style.visibility="visible"}function changeWindow(){setTimeout(function(){setFooter();setTimeout(function(){setFooter()},500)},500)}function startUp(){setFooter();window.addEventListener("onorientationchange" in window?"orientationchange":"resize",changeWindow,false);setTimeout(function(){window.scrollTo(0,1)},300);setInterval(changeWindow,300);}window.onload=startUp;
